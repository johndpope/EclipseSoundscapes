pod 'Localize-Swift'
pod 'BRYXBanner'
pod 'SwiftSpinner'

  target 'EclipseSoundscapesTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'EclipseSoundscapesUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
